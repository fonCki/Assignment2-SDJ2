<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vBox" alignment="CENTER" prefHeight="210.0" prefWidth="510.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.views.settingsView.SettingsViewController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" styleClass="main" stylesheets="@../../../design/style.css">
         <children>
            <AnchorPane prefHeight="110.0" prefWidth="136.0" styleClass="background" stylesheets="@../../../design/style.css">
               <children>
                  <Circle fx:id="circle" fill="WHITE" layoutX="64.0" layoutY="50.0" radius="60.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                  <Button fx:id="openButton" layoutX="4.0" mnemonicParsing="false" prefHeight="110.0" prefWidth="120.0" styleClass="btns" stylesheets="@../../../design/style.css">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </AnchorPane>
            <TextField fx:id="nickNameTextField" promptText="Steve Jobs">
               <font>
                  <Font size="19.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <Button fx:id="onOkButton" mnemonicParsing="false" onAction="#onOkButton" styleClass="dark-blue" stylesheets="@../../../design/style.css" text="LET'S DO IT">
         <font>
            <Font name="Apple Symbols" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
   <padding>
      <Insets top="10.0" />
   </padding>
</VBox>
